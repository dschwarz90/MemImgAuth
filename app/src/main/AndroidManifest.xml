<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.ac.uec.inf.az.memimgauth">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MemImgAuth"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="jp.ac.uec.inf.az.memimgauth.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jp.ac.uec.inf.az.memimgauth.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name="jp.ac.uec.inf.az.memimgauth.showPassImages" />
        <activity android:name="jp.ac.uec.inf.az.memimgauth.authenticate" />
        <activity
            android:name="jp.ac.uec.inf.az.memimgauth.authenticationSuccess"
            android:label="Success!"
            android:logo="@mipmap/ic_launcher"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>